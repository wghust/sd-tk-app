{
  "name": "squirtle",
  "version": "1.0.0",
  "description": "压缩图片app",
  "main": "./main/main.js",
  "scripts": {
    "buildapp": "electron-packager . --icon=logo.icns --platform=darwin",
    "setupmac": "electron-installer-dmg ./squirtle-darwin-x64/squirtle.app squirtle",
    "buildwin": "electron-packager . --icon=logo.ico --platform=win32",
    "setupwin": "electron-installer-windows --src ./psyduck-win32-x64 --dest installers/",
    "app": "electron .",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./build/webpack.config.js",
    "watch": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js -p --progress --hide-modules --watch",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js -p --progress --hide-modules",
    "server": "cross-env NODE_ENV=server gulp connect",
    "upload": "chaos send -c server.json"
  },
  "keywords": [
    "skydragon",
    "sd-luna模板"
  ],
  "author": "zhishui",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
