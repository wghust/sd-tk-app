    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/runtime": "^7.3.1",
    "@skydragon/base": "^2.0.3",
    "@skydragon/breadcrumb": "^2.0.0",
    "@skydragon/menu": "^2.0.2",
    "@skydragon/nav": "^2.0.0",
    "@skydragon/pagination": "^2.0.0",
    "@tbj/chen": "^1.0.2",
    "@tbj/cookie": "^1.1.6",
    "@tbj/dk": "^1.0.14",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "codemirror": "^5.47.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.1.4",
    "css-loader": "^2.1.0",
    "electron": "^2.0.18",
    "electron-installer-dmg": "^2.0.0",
    "electron-installer-windows": "^1.1.1",
    "electron-packager": "^13.1.1",
